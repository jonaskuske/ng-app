    "*.ts": [
      "ng-lint-staged lint --tsConfig=tsconfig.json --fix --"
    ],
    "*.{js,json,css,md,html}": [
      "prettier --write"
    ]
  },
  "eslintIgnore": [
    "*"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
