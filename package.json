{
  "name": "ng-app",
  "version": "2.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint --fix",
    "e2e": "server-test :4200 e2e:run",
    "e2e:open": "cypress open",
    "e2e:run": "cypress run --headless",
    "deploy": "ng deploy",
    "postinstall": "ngcc"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "ng-lint-staged lint --fix --"
    ],
    "*.{js,json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "eslintIgnore": [
    "dist/",
    "coverage/"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.1",
    "@angular/common": "^13.1.1",
    "@angular/compiler": "^13.1.1",
    "@angular/core": "^13.1.1",
    "@angular/forms": "^13.1.1",
    "@angular/platform-browser": "^13.1.1",
    "@angular/platform-browser-dynamic": "^13.1.1",
    "@angular/router": "^13.1.1",
    "@angular/youtube-player": "^13.1.1",
    "@ngxs/devtools-plugin": "^3.7.3",
    "@ngxs/router-plugin": "^3.7.3",
    "@ngxs/storage-plugin": "^3.7.3",
    "@ngxs/store": "^3.7.3",
    "rxjs": "^7.4.0",
    "smoothscroll-anchor-polyfill": "^1.3.3",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.0",
    "@angular-devkit/build-angular": "~16.2.5",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "^13.1.2",
    "@angular/compiler-cli": "^13.1.1",
    "@angular/language-service": "^13.1.1",
    "@cypress/webpack-preprocessor": "^5.10.0",
    "@testing-library/angular": "^11.0.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "angular-cli-ghpages": "*",
    "cypress": "^13.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "jest-preset-angular": "^11.1.1",
    "lint-staged": "^12.0.2",
    "ng-lint-staged": "^12.0.3",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "4.5",
    "yorkie": "^2.0.0"
  }
}
