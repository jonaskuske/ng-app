{
  "name": "ng-app",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng s -c hmr --base-href /ng-app/ -o",
    "build": "ng b --prod",
    "test": "ng t",
    "lint": "ng l --fix",
    "e2e": "ng e"
