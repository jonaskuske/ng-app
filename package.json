{
  "name": "ng-app",
  "version": "2.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng s",
    "build": "ng b --configuration production",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "test:watch": "npx jest --watch",
    "lint": "ng l --fix",
    "e2e": "run-p -r start e2e:run",
    "e2e:open": "cypress open",
    "e2e:run": "cypress run --headless",
    "postinstall": "ngcc"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "ng-lint-staged lint --tsConfig=tsconfig.json --fix --"
    ],
    "*.{js,json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "eslintIgnore": [
    "*"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.13",
    "@angular/common": "^12.2.13",
    "@angular/compiler": "^12.2.13",
    "@angular/core": "^12.2.13",
    "@angular/forms": "^12.2.13",
    "@angular/platform-browser": "^12.2.13",
    "@angular/platform-browser-dynamic": "^12.2.13",
    "@angular/router": "^12.2.13",
    "@angular/youtube-player": "^12.2.13",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@ngxs/router-plugin": "^3.7.1",
    "@ngxs/storage-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.1",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "rxjs": "~6.6.3",
    "smoothscroll-anchor-polyfill": "^1.3.2",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.0.0",
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "^12.2.13",
    "@angular/compiler-cli": "^12.2.13",
    "@angular/language-service": "^12.2.13",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@testing-library/angular": "^10.11.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "codelyzer": "^6.0.0",
    "cypress": "^6.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "ng-lint-staged": "0.1.6",
    "npm-run-all": "4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsconfig-paths-jest": "0.0.1",
    "tslint": "^6.1.3",
    "tslint-angular": "^3.0.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.3.5",
    "yorkie": "^2.0.0"
  }
}
