{
  "name": "ng-app",
  "version": "2.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng s",
    "build": "ng b --configuration production",
    "test": "ng test",
    "test:coverage": "npx jest --coverage",
    "test:watch": "npx jest --watch",
    "lint": "ng l --fix",
    "e2e": "run-p -r start e2e:run",
    "e2e:open": "cypress open",
    "e2e:run": "cypress run --headless",
    "postinstall": "ngcc"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "ng-lint-staged lint --tsConfig=tsconfig.json --fix --"
    ],
    "*.{js,json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "eslintIgnore": [
    "*"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.13",
    "@angular/common": "^12.2.13",
    "@angular/compiler": "^12.2.13",
    "@angular/core": "^12.2.13",
    "@angular/forms": "^12.2.13",
    "@angular/platform-browser": "^12.2.13",
    "@angular/platform-browser-dynamic": "^12.2.13",
    "@angular/router": "^12.2.13",
    "@angular/youtube-player": "^12.2.13",
    "@ngxs/devtools-plugin": "^3.7.2",
    "@ngxs/router-plugin": "^3.7.2",
    "@ngxs/storage-plugin": "^3.7.2",
    "@ngxs/store": "^3.7.2",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "rxjs": "^6.6.7",
    "smoothscroll-anchor-polyfill": "^1.3.3",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^12.1.2",
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "^12.2.13",
    "@angular/compiler-cli": "^12.2.13",
    "@angular/language-service": "^12.2.13",
    "@cypress/webpack-preprocessor": "^5.10.0",
    "@testing-library/angular": "^10.11.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "codelyzer": "^6.0.2",
    "cypress": "^9.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "ng-lint-staged": "^7.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths-jest": "0.0.1",
    "tslint": "^6.1.3",
    "tslint-angular": "^3.0.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.3.5",
    "yorkie": "^2.0.0"
  }
}
