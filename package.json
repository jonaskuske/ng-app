{
  "name": "ng-app",
  "version": "1.4.2",
  "scripts": {
    "ng": "ng",
    "start": "ng s -c hmr",
    "build": "ng b --prod",
    "deploy": "ng b --prod --output-path docs --base-href /ng-app/",
    "test": "ng t",
    "lint": "ng l --fix",
    "e2e": "ng e"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "ng-lint-staged lint --tsConfig=tsconfig.json --fix --",
      "git add"
    ],
    "*.{js,json,css,md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintIgnore": [
    "*"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/common": "^8.2.13",
    "@angular/compiler": "^8.2.13",
    "@angular/core": "^8.2.13",
    "@angular/forms": "^8.2.13",
    "@angular/platform-browser": "^8.2.13",
    "@angular/platform-browser-dynamic": "^8.2.13",
    "@angular/router": "^8.2.13",
    "@angular/youtube-player": "^8.2.3",
    "@ngxs/devtools-plugin": "^3.5.1",
    "@ngxs/router-plugin": "3.5.0",
    "@ngxs/storage-plugin": "^3.5.1",
    "@ngxs/store": "^3.5.1",
    "core-js": "^3.3.5",
    "rxjs": "~6.5.3",
    "smoothscroll-anchor-polyfill": "^1.3.0",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.16",
    "@angular/cli": "^8.3.16",
    "@angular/compiler-cli": "^8.2.13",
    "@angular/language-service": "^8.2.13",
    "@angularclass/hmr": "^2.1.3",
    "@ngxs/hmr-plugin": "^3.5.1",
    "@types/jasmine": "^3.4.4",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.3",
    "codelyzer": "^5.2.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^9.4.2",
    "ng-lint-staged": "^0.1.6",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.5.3",
    "yorkie": "^2.0.0"
  }
}
